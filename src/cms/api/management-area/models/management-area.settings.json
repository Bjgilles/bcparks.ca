{
  "kind": "collectionType",
  "collectionName": "management_areas",
  "info": {
    "name": "ManagementArea",
    "description": "Management Area"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "managementAreaNumber": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "managementAreaName": {
      "type": "string"
    },
    "section": {
      "model": "section",
      "via": "managementAreas"
    },
    "protectedAreas": {
      "collection": "protected-area",
      "via": "managementAreas",
      "dominant": true
    },
    "region": {
      "via": "managementAreas",
      "model": "region"
    }
  }
}
